<server description="new server">

	<featureManager>
		<feature>webProfile-7.0</feature>
		<feature>localConnector-1.0</feature>
	</featureManager>

	<httpEndpoint host="*" httpPort="9080" httpsPort="9443" id="defaultHttpEndpoint"/>

	<applicationManager autoExpand="true"/>

	<logging traceSpecification="*=info"/>

	<keyStore id="defaultKeyStore" password="WebAsWebAS"/>


	<dataSource jndiName="jdbc/mileit">
		<properties databaseName="mileit" password="mileitpwd" portNumber="3306" serverName="0.0.0.0" user="mileit"/>
	</dataSource>

	<applicationMonitor updateTrigger="mbean"/>
	<basicRegistry>
		<user name="user1" password="password"/>
		<group name="users">
			<member name="user1"/>
		</group>
	</basicRegistry>

	<webApplication id="MileIT" location="MileIT.war" name="MileIT"/>
</server>